{
  "name": "hatsune-miku-theme",
  "displayName": "Hatsune Miku Theme",
  "publisher": "vauxe",
  "description": "All-Miku Synthesis: A high-fidelity VS Code theme integrating every Miku version (V2 to SEKAI) with ergonomic color mapping for marathon coding sessions.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vauxe/hatsune-miku-theme.git"
  },
  "version": "3.9.39",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "build": "tsc && node dist/generator.js",
    "build:theme": "node dist/generator.js",
    "watch": "tsc -w",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3"
  },
  "contributes": {
    "themes": [
      {
        "label": "Hatsune Miku Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Hatsune Miku Theme-color-theme.json"
      }
    ]
  }
}