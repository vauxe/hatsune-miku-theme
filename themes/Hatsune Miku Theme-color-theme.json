	"tokenColors": [
		{
			"name": "Comments (Silent/Invisible)",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#455A64"
			}
		},
		{
			"name": "Keywords (Identity - Miku Teal)",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#39C5BB",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators & Punctuation (Structure - Transparent Teal)",
			"scope": [
				"keyword.operator",
				"punctuation",
				"meta.brace",
				"meta.delimiter",
				"meta.group.braces"
			],
			"settings": {
				"foreground": "#39C5BB90",
				"fontStyle": ""
			}
		},
		{
			"name": "Functions & Methods (Voice - Bright Cyan)",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic",
				"meta.function-call.python"
			],
			"settings": {
				"foreground": "#4DD0E1",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables (Data - Metallic Grey)",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"meta.object-literal.key",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#CFD8DC",
				"fontStyle": ""
			}
		},
		{
			"name": "Member Properties/Fields (Interface - Lighter Grey)",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"meta.property.object"
			],
			"settings": {
				"foreground": "#B0BEC5"
			}
		},
		{
			"name": "Types, Classes, Interfaces (Structure - Amber)",
			"scope": [
				"entity.name.type",
				"support.type",
				"support.class",
				"entity.other.inherited-class",
				"entity.name.class",
				"entity.name.namespace",
				"entity.name.module"
			],
			"settings": {
				"foreground": "#FFD740",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constants, Numbers, Booleans (Rhythm - Magenta)",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"constant.other.caps"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": ""
			}
		},
		{
			"name": "Strings (Content - Negi Green)",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#9CCC65"
			}
		},
		{
			"name": "Decorators / Annotations (Meta - Amber Italic)",
			"scope": [
				"meta.decorator",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#FFD740",
				"fontStyle": "italic"
			}
		},
		{
			"name": "RegEx / Errors (Feature - Tattoo Red)",
			"scope": [
				"string.regexp",
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON/YAML Keys (Structure)",
			"scope": [
				"support.type.property-name",
				"entity.name.tag.yaml",
				"constant.other.key",
				"source.json meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#39C5BB",
				"fontStyle": ""
			}
		},
		{
			"name": "HTML/XML Tags (Structure)",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#39C5BB",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML/XML Attributes (Details - Magenta Italic)",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Python Self/Cls (Special - Magenta Italic)",
			"scope": [
				"variable.language.special.self",
				"variable.parameter.function.language.special.self",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Headings (Structure - Pink/Teal)",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Links (Link - Cyan)",
			"scope": [
				"markup.underline.link",
				"mod_link"
			],
			"settings": {
				"foreground": "#4DD0E1"
			}
		},
		{
			"name": "Markdown Code (Content - Negi Green)",
			"scope": [
				"markup.inline.raw",
				"markup.raw.block"
			],
			"settings": {
				"foreground": "#9CCC65"
			}
		},
		{
			"name": "CSS Classes (Structure - Teal)",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#39C5BB"
			}
		},
		{
			"name": "CSS IDs (Rhythm - Pink)",
			"scope": [
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": "bold"
			}
		}
	],
	"semanticTokenColors": {
		"function": {
			"foreground": "#4DD0E1",
			"fontStyle": "italic"
		},
		"method": {
			"foreground": "#4DD0E1",
			"fontStyle": "italic"
		},
		"class": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"struct": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"parameter": "#CFD8DC",
		"variable": "#CFD8DC",
		"property": "#B0BEC5",
		"type": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"interface": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"namespace": "#39C5BB",
		"enumMember": "#E05096",
		"comment": {
			"foreground": "#455A64",
			"fontStyle": "italic"
		},
		"string": "#9CCC65",
		"number": "#E05096",
		"regexp": "#FF5370",
		"operator": "#39C5BB90"
	}
}