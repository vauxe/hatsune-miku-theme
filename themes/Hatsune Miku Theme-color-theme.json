{
	"name": "Hatsune Miku Theme",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#1A1F24",
		"editor.foreground": "#CFD8DC",
		"editorCursor.foreground": "#E05096",
		"editorCursor.background": "#1A1F24",
		"editor.lineHighlightBackground": "#39C5BB10",
		"editor.lineHighlightBorder": "#39C5BB00",
		"editor.selectionBackground": "#39C5BB33",
		"editor.selectionHighlightBackground": "#39C5BB20",
		"editor.wordHighlightBackground": "#39C5BB20",
		"editor.wordHighlightStrongBackground": "#E0509630",
		"editor.findMatchBackground": "#E0509650",
		"editor.findMatchHighlightBackground": "#E0509620",
		"editorLineNumber.foreground": "#39C5BB40",
		"editorLineNumber.activeForeground": "#39C5BB",
		"editorIndentGuide.background": "#39C5BB10",
		"editorIndentGuide.activeBackground": "#39C5BB60",
		"editorRuler.foreground": "#39C5BB10",
		"editorWhitespace.foreground": "#39C5BB15",
		"editorBracketMatch.background": "#39C5BB30",
		"editorBracketMatch.border": "#39C5BB",
		"editorGutter.addedBackground": "#39C5BB80",
		"editorGutter.modifiedBackground": "#FFD74080",
		"editorGutter.deletedBackground": "#FF537080",
		"editorWidget.background": "#1A1F24",
		"editorWidget.border": "#39C5BB60",
		"editorHoverWidget.background": "#1E2529",
		"editorHoverWidget.border": "#39C5BB40",
		"activityBar.background": "#15191D",
		"activityBar.foreground": "#39C5BB",
		"activityBar.activeBorder": "#39C5BB",
		"activityBar.inactiveForeground": "#546E7A",
		"activityBarBadge.background": "#E05096",
		"activityBarBadge.foreground": "#FFFFFF",
		"sideBar.background": "#15191D",
		"sideBar.foreground": "#B0BEC5",
		"sideBar.border": "#39C5BB10",
		"sideBarSectionHeader.background": "#1E2529",
		"sideBarSectionHeader.foreground": "#39C5BB",
		"sideBarTitle.foreground": "#39C5BB",
		"statusBar.background": "#15191D",
		"statusBar.foreground": "#CFD8DC",
		"statusBar.border": "#39C5BB20",
		"statusBar.debuggingBackground": "#E05096",
		"statusBar.debuggingForeground": "#FFFFFF",
		"statusBar.noFolderBackground": "#15191D",
		"statusBarItem.remoteBackground": "#39C5BB",
		"statusBarItem.remoteForeground": "#15191D",
		"titleBar.activeBackground": "#15191D",
		"titleBar.activeForeground": "#CFD8DC",
		"titleBar.inactiveBackground": "#15191D",
		"titleBar.inactiveForeground": "#546E7A",
		"tab.activeBackground": "#1A1F24",
		"tab.activeForeground": "#39C5BB",
		"tab.activeBorderTop": "#E05096",
		"tab.inactiveBackground": "#15191D",
		"tab.inactiveForeground": "#546E7A",
		"tab.border": "#15191D00",
		"list.activeSelectionBackground": "#39C5BB30",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.hoverBackground": "#39C5BB15",
		"list.focusBackground": "#39C5BB20",
		"list.highlightForeground": "#39C5BB",
		"focusBorder": "#39C5BB60",
		"input.background": "#1E2529",
		"input.foreground": "#CFD8DC",
		"input.border": "#39C5BB40",
		"input.placeholderForeground": "#546E7A",
		"inputOption.activeBorder": "#E05096",
		"dropdown.background": "#1E2529",
		"dropdown.foreground": "#CFD8DC",
		"dropdown.border": "#39C5BB40",
		"terminal.background": "#1A1F24",
		"terminal.foreground": "#CFD8DC",
		"terminal.ansiBlack": "#15191D",
		"terminal.ansiRed": "#FF5370",
		"terminal.ansiGreen": "#9CCC65",
		"terminal.ansiYellow": "#FFD740",
		"terminal.ansiBlue": "#4DD0E1",
		"terminal.ansiMagenta": "#E05096",
		"terminal.ansiCyan": "#39C5BB",
		"terminal.ansiWhite": "#CFD8DC",
		"terminal.ansiBrightBlack": "#455A64",
		"terminal.ansiBrightRed": "#FF5370",
		"terminal.ansiBrightGreen": "#AED581",
		"terminal.ansiBrightYellow": "#FFE57F",
		"terminal.ansiBrightBlue": "#80DEEA",
		"terminal.ansiBrightMagenta": "#F06292",
		"terminal.ansiBrightCyan": "#26A69A",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"widget.shadow": "#00000060",
		"selection.background": "#39C5BB40",
		"descriptionForeground": "#546E7A",
		"textLink.foreground": "#4DD0E1",
		"textLink.activeForeground": "#39C5BB"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#455A64"
			}
		},
		{
			"name": "Keywords, Storage, Structure",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#39C5BB",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators (Dimmed Structure)",
			"scope": [
				"keyword.operator",
				"punctuation",
				"meta.brace",
				"meta.delimiter"
			],
			"settings": {
				"foreground": "#39C5BB80",
				"fontStyle": ""
			}
		},
		{
			"name": "Functions, Methods (Voice/Action)",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic",
				"meta.function-call.python"
			],
			"settings": {
				"foreground": "#4DD0E1",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables (Data)",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#CFD8DC",
				"fontStyle": ""
			}
		},
		{
			"name": "Types, Classes (Definitions)",
			"scope": [
				"entity.name.type",
				"support.type",
				"support.class",
				"entity.other.inherited-class",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#FFD740",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constants, Numbers, Booleans (Rhythm)",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": ""
			}
		},
		{
			"name": "Strings (Negi/Content)",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#9CCC65"
			}
		},
		{
			"name": "Decorators / Annotations",
			"scope": [
				"meta.decorator",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#FFD740",
				"fontStyle": "italic"
			}
		},
		{
			"name": "RegEx / Errors",
			"scope": [
				"string.regexp",
				"invalid"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON/YAML Keys",
			"scope": [
				"support.type.property-name",
				"entity.name.tag.yaml",
				"constant.other.key"
			],
			"settings": {
				"foreground": "#39C5BB",
				"fontStyle": ""
			}
		},
		{
			"name": "HTML/XML Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#39C5BB",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML/XML Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Python Specific: Self/Cls",
			"scope": [
				"variable.language.special.self",
				"variable.parameter.function.language.special.self",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#E05096",
				"fontStyle": "italic"
			}
		}
	],
	"semanticTokenColors": {
		"function": {
			"foreground": "#4DD0E1",
			"fontStyle": "italic"
		},
		"method": {
			"foreground": "#4DD0E1",
			"fontStyle": "italic"
		},
		"class": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"parameter": "#CFD8DC",
		"variable": "#CFD8DC",
		"property": "#CFD8DC",
		"type": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"interface": {
			"foreground": "#FFD740",
			"fontStyle": "bold"
		},
		"namespace": "#39C5BB",
		"enumMember": "#E05096",
		"comment": {
			"foreground": "#455A64",
			"fontStyle": "italic"
		}
	}
}